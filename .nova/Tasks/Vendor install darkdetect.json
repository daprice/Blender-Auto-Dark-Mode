{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\npip install --target=\"$1\/src\/vendor\" --upgrade darkdetect"
    },
    "clean" : {
      "enabled" : true,
      "script" : "#!\/bin\/sh\nrm -r src\/vendor\/*"
    }
  },
  "arguments" : [
    "${WorkspaceFolder}"
  ],
  "openLogOnRun" : "start"
}
